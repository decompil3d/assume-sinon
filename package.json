{
  "name": "assume-sinon",
  "version": "1.0.0",
  "description": "Extends assume with assertions for the Sinon.JS mocking framework",
  "main": "index.js",
  "scripts": {
    "test": "mocha $(find test -name '*.test.js')",
    "watch": "mocha --watch $(find test -name '*.test.js')",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- $(find test -name '*.test.js')",
    "test-travis": "istanbul cover node_modules/.bin/_mocha --report lcovonly -- $(find test -name '*.test.js')"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/terinjokes/assume-sinon"
  },
  "keywords": [
    "sinon",
    "assume",
    "testing",
    "spies",
    "stubs",
    "mocks"
  ],
  "author": "Terin Stock <terinjokes@gmail.com> (https://terinstock.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/terinjokes/assume-sinon/issues"
  },
  "homepage": "https://github.com/terinjokes/assume-sinon",
  "devDependencies": {
    "assume": "1.1.x",
    "istanbul": "0.3.x",
    "mocha": "2.1.x",
    "sinon": "1.12.x",
    "pre-commit": "1.0.x"
  }
}
